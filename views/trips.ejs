      window.dataLayer = window.dataLayer || [];
      function gtag(){dataLayer.push(arguments);}
      gtag('js', new Date());
      gtag('config', 'UA-93215177-2');
    </script>

</head>
<body>
<% include partials/navbar.ejs %>
<section id="voomsway" class="main-section"
         data-apikey="<%= process.env.VOOMSWAY_API_KEY %>"
         data-apiUrl="<%= process.env.VOOMSWAY_API_HOST %>"
         data-baseUrl="<%= process.env.HOST %>"
         data-verifyRedirect="<%= process.env.HOST %>/vway/verify-link"
         data-passwordRedirect="<%= process.env.HOST %>/vway/update-password"
         data-fbid="<%= (social_auth_keys) ? social_auth_keys.facebook_app_id : '' %>"
         data-gid="<%= (social_auth_keys) ? social_auth_keys.google_api_key : '' %>"
         data-gcid="<%= (social_auth_keys) ? social_auth_keys.google_client_id : '' %>"
         data-paystack="<%= process.env.PAYSTACK_API_KEY %>"
         data-mixpanel="<%= process.env.MIXPANEL_TOKEN %>"
></section>

<footer class="container-fluid shadow ">
    <div class="container">
        <div class="row">
            <div class="col-lg-3 col-sm-12 mr-2">
                <a class="navbar-brand footer-logo" href="/"><img src="/images/DivineExpress.png" style="height: 60px;"></a>
            </div>
            <div class="col-lg-6 col-sm-12 mt-4 text-white justify-content-center">
                <ul class="list-inline ">
                    <li class="list-inline-item mr-3 "><a href="/vway/trips">BOOK TRIPS</a></li>
                    <li class="list-inline-item mr-3 "><a href="/terminals">TERMINALS</a></li>
                    <li class="list-inline-item mr-3 "><a href="/about">ABOUT</a></li>
                    <li class="list-inline-item mr-3 "><a href="/contact">CONTACT</a></li>
                    <li class="list-inline-item mr-3 "><a href="/terms">TERMS & CONDITION</a></li>
                </ul>
            </div>
            <div class="col-lg-2 col-sm-12 mt-4 text-white justify-content-center">
                <a href="https://www.facebook.com/divinextransport/" target="_blank"><i
                            class="fab fa-facebook text-white fa-2x mr-3"></i></a>
                <a href="https://twitter.com/divine_express1" target="_blank"><i
                            class="fab fa-twitter text-white fa-2x mr-3"></i></a>
                <a href="https://www.instagram.com/divinextransport/" target="_blank"><i
                            class="fab fa-instagram text-white fa-2x"></i></a>
            </div>
        </div>
    </div>
    <div class="vm-footer d-flex justify-content-center border-top border-dark align-self-center">
        <p>Â© Copyright 2019 Divine Express Transport Company</p>
    </div>
</footer>


